{
  "version": 2,
  "name": "dandori-work-ai-agent",
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/health", "destination": "/api" },
    { "source": "/webhook", "destination": "/api" },
    { "source": "/events", "destination": "/api" },
    { "source": "/(.*)", "destination": "/api" }
  ]
}